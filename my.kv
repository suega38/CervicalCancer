<PredictWindow>:
    name: "predict"
    age: age
    smokeDuration:smokeDuration
    pregnancy:pregnancy
    sexualPartners:sexualPartners
    Label:
        text: "Predict" 
        bold: True
        pos_hint: {"x":0.35, "y":0.81}
        font_size: 23 

    Button: 
        background_color: (1,1,1,1)
        pos_hint: {"x":0, "y": 0.92}
        size_hint: (0.07, 0.07)
        on_release: app.root.current = "home"
        canvas.before:
            Color:
                rgba: (1,1,1,1)
            Rectangle:
                size: self.size
                pos: self.pos
        Image:
            source: 'images/arrow.png'
            center_x: self.parent.center_x
            center_y: self.parent.center_y
    Label:
        text: "Please fill in this questionnaire to generate prediction result. " 
        pos_hint: {"x":0.35, "y":0.73}
        font_size: 12
    ScrollView:
        do_scroll_y: True
        do_scroll_x: False
        size_hint: (0.85,0.6)
        font_size: 10
        pos_hint:{'center_x':0.5, 'center_y':0.5}
        GridLayout:
            size_hint_y: None
            height:self.minimum_height
            width: self.minimum_width
            cols:1
            spacing:15
            size_hint: 1, None
            height:1000
            width:90
            Label:
                font_size: 16
                text: "How old are you? "
                color: (0,0,0,1)
                canvas:
                    # draw a background of teal. This will be the border
                    Color:
                        rgba: (0/255.0,128/255.0,128/255.0,0.3)
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
            TextInput:
                size_hint: 0.45, 0.2
                id: age
                multiline: False
                input_type: 'number'
                input_filter: 'int'

            Label:
                text: "Do you smoke or have you ever smoke? "
                font_size: 16
                color: (0,0,0,1)
                canvas:
                    # draw a background of teal. This will be the border
                    Color:
                        rgba: (0/255.0,128/255.0,128/255.0,0.3)
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
            BoxLayout:
                #color: (0,0,0,1)
                height:self.minimum_height
                width: self.minimum_width
                orientation: 'horizontal'
                size_hint: 0.3,0.17
                cols:2
                canvas:
                    #  This will be the border
                    Color:
                        rgba: (161/255.0,176/255.0,181/255.0,0.3)
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                Label:
                    color: (0,0,0,1)
                    pos_hint:{'center_x':.5,'y':0.3}
                    font_size: 14
                    text: "No "
                CheckBox:
                    id: smoke
