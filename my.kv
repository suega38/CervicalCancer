                multiline: False

            Label:
                pos_hint: {"x":0.075, "top":0.61}
                text: "Password: "

            TextInput:
                pos_hint: {"x":0.4, "top":0.49}
                id: passw
                multiline: False
                password: True
                
            Label:
                pos_hint: {"x":0.075, "top":0.54}
                text: "Phone Number: "

            TextInput:
                pos_hint: {"x":0.4, "top":0.42}
